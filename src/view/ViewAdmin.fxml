            <Font name="Arial Bold" size="16.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <Button fx:id="btnProduct" layoutX="87.0" layoutY="161.0" mnemonicParsing="false" onAction="#product" style="-fx-background-color: #32454c;" text="Administrar productos" textFill="WHITE">
         <font>
            <Font name="Arial Bold" size="16.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <Button fx:id="btnCancel" layoutX="145.0" layoutY="238.0" mnemonicParsing="false" onAction="#cancel" style="-fx-background-color: #ff4040;" text="Cacelar" textFill="WHITE">
