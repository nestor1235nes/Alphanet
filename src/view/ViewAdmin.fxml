            <Image url="@../Images/admin.png" />
         </image>
      </ImageView>
      <Button fx:id="btnCashier" layoutX="98.0" layoutY="100.0" mnemonicParsing="false" onAction="#cashier" style="-fx-background-color: #32454c;" text="Administrar cajeros" textFill="WHITE">
