            <Font name="Baskerville Old Face" size="18.0" />
         </font>
      </Text>
      <Button fx:id="btnStart" layoutX="181.0" layoutY="87.0" mnemonicParsing="false" onAction="#start" style="-fx-background-color: #32454c;" text="Ingresar" textFill="WHITE">
         <font>
            <Font name="Baskerville Old Face" size="16.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Button>
   </children>
</AnchorPane>
