            <Image url="@../Images/caja.png" />
         </image>
      </ImageView>
      <TextField fx:id="txtMonto" layoutX="254.0" layoutY="30.0" />
      <Text layoutX="34.0" layoutY="47.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Ingrese monto de la caja">
